{
    "name": "navno-search",
    "version": "1.0.0",
    "description": "Ye olde nav.no search",
    "scripts": {
        "build": "npm-run-all lint:* build:*",
        "build:transpile-local": "babel src/main/resources --out-dir=build/resources/main --ignore \"**/*.js\"",
        "lint:backend": "eslint --ext=es6 ./src/main/resources --quiet",
        "lint-staged": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/navno-search.git"
    },
    "license": "MIT",
    "lint-staged": {
        "*.es6": "eslint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "homepage": "https://github.com/navikt/nav-enonicxp-search#readme",
    "devDependencies": {
        "@babel/cli": "7.19.3",
        "@babel/core": "7.20.5",
        "@babel/preset-env": "7.20.2",
        "core-js": "3.26.1",
        "eslint": "8.28.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "4.3.8",
        "lint-staged": "10.5.4",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.0"
    },
    "dependencies": {}
}
