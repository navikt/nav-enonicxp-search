{
    "name": "navno-search",
    "version": "1.0.0",
    "description": "Ye olde nav.no search",
    "scripts": {
        "build": "npm-run-all lint:* build:*",
        "watch": "npm-run-all watch:*",
        "dev": "npm-run-all -p watch:lint watch:transpile-local",
        "build:transpile-local": "babel src/main/resources --out-dir=build/resources/main --ignore \"**/*.js\"",
        "watch:lint": "onchange \"src/main/resources/**/*.es6\" -- npm run lint --silent",
        "watch:transpile-local": "babel src/main/resources --watch --extensions '.es6' --out-dir=build/resources/main",
        "lint:backend": "eslint --ext=es6 ./src/main/resources --quiet",
        "lint-staged": "lint-staged"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/navno-search.git"
    },
    "license": "MIT",
    "lint-staged": {
        "*.es6": "eslint"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "homepage": "https://github.com/navikt/nav-enonicxp-search#readme",
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/preset-env": "^7.10.2",
        "core-js": "^3.6.5",
        "eslint": "^7.2.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-standard": "^4.0.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.9",
        "npm-run-all": "^4.1.5",
        "onchange": "^7.0.2",
        "prettier": "^2.0.5",
        "prompt": "^1.0.0",
        "replace": "^1.2.0",
        "serve": "^11.3.2"
    },
    "dependencies": {}
}
