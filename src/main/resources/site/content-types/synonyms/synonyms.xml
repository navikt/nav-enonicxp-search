<content-type>
    <display-name>Synonymer</display-name>
    <description>Skal bare finnes 1 av denne</description>
    <is-abstract>true</is-abstract>
    <super-type>base:unstructured</super-type>
    <form>
        <item-set name="synonyms">
            <label>Synonymer</label>
            <occurrences maximum="0" minimum="0"/>
            <items>
                <input name="synonym" type="TextLine">
                    <label>Liste over synonymer</label>
                    <occurrences maximum="0" minimum="2"/>
                </input>
            </items>
        </item-set>
    </form>
</content-type>
