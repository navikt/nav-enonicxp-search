<mixin>
    <display-name>Key value pairs</display-name>
    <items>
    <item-set name="keyVals">
    <label>Egenskaper</label>
    <occurrences minimum="0" maximum="0"/>
    <items>
        <input name="key" type="TextLine">
            <label>NÃ¸kkel</label>
            <occurrences maximum="1" minimum="1"/>
        </input>
        <item-set name="value">
            <label>Verdier</label>
            <occurrences minimum="1" maximum="1"/>
            <items>
                <input name="string" type="TextLine">
                    <label>String</label>
                </input>
                <input type="Double" name="number">
                    <label>Number</label>
                </input>
                <input name="boolean" type="RadioButton">
                    <label>Boolean</label>
                    <config>
                        <option value="true">True</option>
                        <option value="false">False</option>
                    </config>
                </input>
                <input name="array" type="Tag">
                    <label>Array</label>
                    <occurrences maximum="0" minimum="0"/>
                </input>
            </items>
        </item-set>
    </items>
    </item-set>
    </items>
</mixin>
